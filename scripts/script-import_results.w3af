# demo that imports results from a file

plugins
output console,text_file
output config text_file
set fileName output-w3af.txt
set verbose True
back
output config console
set verbose False
back

discovery import_results
crawl config import_results
set input_csv scripts/input-test.csv
back

back

target
set target http://moth/w3af/audit/xss/simple_xss.php
back

start

assert len(kb.kb.getData('urls','url_objects')) == 4

exit
